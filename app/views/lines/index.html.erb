<h3>LINE@</h3>
<table class="table-container">
  <thead>
    <tr>
      <th scorp ="col">No.</th>
      <th scorp ="col">ジャンル</th>
      <th scorp ="col">内容</th>
    </tr>
  </thead>
  <tbody>
    <% @lines.each.with_index(1) do |line, i|%>
      <tr>
        <th scorp ="row"><%= i %></th>
        <td><%= line.genre %></td>
        <td><%= link_to line.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>